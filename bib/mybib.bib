	keywords = {Gene Expression Profiling, Animals, Mice, Algorithms, Sequence Analysis, RNA, Gene Expression Regulation, Models, Statistical, Poisson Distribution},
	pages = {S14},
	file = {Full Text:/home/nikos/Zotero/storage/6YNWKPLY/Chen et al. - 2015 - Differential expression analysis of RNA sequencing.pdf:application/pdf}
}

@article{robles_efficient_2012,
