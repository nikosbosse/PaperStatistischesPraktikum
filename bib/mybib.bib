	year = {2010},
	keywords = {Foreign exchange, Mean reversion, Momentum, Trading strategies, Uncovered interest parity},
	pages = {2720--2727},
	file = {ScienceDirect Snapshot:/home/nikos/Zotero/storage/52H6S9H7/S0378426610001883.html:text/html}
}

